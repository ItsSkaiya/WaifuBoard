{
  "name": "waifu-board",
  "version": "v1.5.0",
  "description": "An image board app to store and browse your favorite waifus!",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "rebuild": "npx electron-rebuild",
    "build": "npx electron-builder",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "electron": "^39.2.1",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "@ryuziii/discord-rpc": "^1.0.1-rc.1",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "node-fetch": "^2.7.0",
    "sqlite3": "^5.1.7"
  },
  "build": {
    "appId": "com.skylar.waifuboard",
    "productName": "Waifu Board",
